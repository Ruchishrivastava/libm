<p id="notice"><%= notice %></p>

<h3><strong>Subjects<strong></h3>

<table>
  <thead>
    <tr>
      <th>Subject name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_subjects.each do |admin_subject| %>
      <tr>
        <td><%= admin_subject.subject_name %></td>

      
        <td><%= link_to 'Show', admin_subject %></td>

        <td><%= link_to 'Edit', edit_admin_subject_path(admin_subject) %></td>
        <td><%= link_to 'Destroy', admin_subject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
       
      

      </tr>
    <% end %>
 
     <%= link_to 'New Subject', new_admin_subject_path %>
    
  </tbody>
</table>

<br>



